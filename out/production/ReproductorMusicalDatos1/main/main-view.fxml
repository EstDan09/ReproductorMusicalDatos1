<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="739.0" prefWidth="895.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="inicioCorreo" promptText="Correo electrónico" />
   <PasswordField fx:id="inicioContrasena" promptText="Contraseña" />
    <Button fx:id="iSesion" onAction="#goToCrear" prefHeight="28.0" prefWidth="118.0" text="Iniciar Sesión" />
   <Button fx:id="crearCuenta" onAction="#goToCrear" prefHeight="28.0" prefWidth="118.0" text="Crear Cuenta" />
</VBox>
